{
  "name": "{{name}}",
  "version": "1.0.0",
  "description": "{{description}}",
  "author": "{{author}}",
  "type": "commonjs",
  "dependencies": {
  },
  "devDependencies": {
    "autod": "^3",
    "egg-bin": "^4",
    "egg-ci": "^1",
    "eslint": "^8",
    "eslint-config-egg": "^11"
  },
  "scripts": {
    "autod": "autod",
    "lint": "eslint .",
    "test": "npm run lint -- --fix && egg-bin pkgfiles && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "ci": "npm run lint && egg-bin pkgfiles --check && npm run cov",
    "pkgfiles": "egg-bin pkgfiles"
  },
  "files": [
    "lib",
    "index.js"
  ],
  "repository": {
    "type": "git",
    "url": ""
  },
  "publishConfig": {
    "access": "public"
  },
  "ci": {
    "version": "16",
    "type": "github"
  },
  "eslintConfig": {
    "extends": "eslint-config-egg",
    "ignorePatterns": [
      "dist",
      "coverage",
      "node_modules"
    ]
  },
  "license": "MIT"
}
